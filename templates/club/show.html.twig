{% extends 'base.html.twig' %}

{% block title %}Club{% endblock %}

{% block body %}
    <h1>Club</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ club.id }}</td>
            </tr>
            <tr>
                <th>Nomclub</th>
                <td>{{ club.nomclub }}</td>
            </tr>
            <tr>
                <th>Categorie</th>
                <td>{{ club.categorie }}</td>
            </tr>
            <tr>
                <th>Datecreation</th>
                <td>{{ club.datecreation }}</td>
            </tr>
            <tr>
                <th>Nomcoach</th>
                <td>{{ club.nomcoach }}</td>
            </tr>
            <tr>
                <th>Nbmembres</th>
                <td>{{ club.nbmembres }}</td>
            </tr>
            <tr>
                <th>Localisation</th>
                <td>{{ club.localisation }}</td>
            </tr>
            <tr>
                <th>Descriptionclub</th>
                <td>{{ club.descriptionclub }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ club.logo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_club_index') }}">back to list</a>

    <a href="{{ path('app_club_edit', {'id': club.id}) }}">edit</a>

    {{ include('club/_delete_form.html.twig') }}
{% endblock %}
