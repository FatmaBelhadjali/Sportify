<body>
    <!-- Header -->
    <header class="site-navbar py-4" role="banner">
        <div class="container">
            <div class="d-flex align-items-center">
                <div class="site-logo" style="margin-top: 20px;">
                    <!-- Insérez ici le contenu du logo -->
                </div>

                <div class="ml-auto">
                    <nav class="site-navigation position-relative text-right" role="navigation">
                        <ul class="site-menu main-menu js-clone-nav mr-auto d-none d-lg-block">
                            <li class="active"><a href="{{ path("app_sport_indexfront") }}" class="nav-link">SPORTS</a></li>
                            <li><a href="players.html" class="nav-link">CLUBS</a></li>
                            <li><a href="players.html" class="nav-link">PRODUCTS</a></li>
