    {% for club in clubs %}
        <div class="col-lg-6 mb-4">
            <div class="bg-light p-4 rounded">
                <div class="widget-body">
                    <div class="widget-vs">
                        <div class="bg-light p-4 rounded d-flex align-items-center justify-content-center">
                            <div class="team-1 text-center">
                                <img src="{{ club.logo }}" alt="Image" style="width: 150px; height: 100px;">
                                <h3>{{ club.nomclub }}</h3>
                            </div>
                            <div class="team-2 text-center">
                                <!-- Ajoutez ici les détails de l'autre équipe si nécessaire -->
                            </div>
                        </div>
                    </div>
                </div>
                <div class="text-center widget-vs-contents mb-4">
                    <h4>{{ club.categorie }}</h4>
                    <p class="mb-5">
                        <span class="d-block">{{ club.datecreation }}</span>
                        <span class="d-block">{{ club.nbmembres }}</span>
                        <strong class="text-primary">{{ club.nomcoach }}</strong>
                    </p>
                    <p><a href={{ path('app_affiliation_new') }} class="btn btn-primary">Join</a></p>
                </div>
            </div>
        </div>
        
    {% endfor %}


    

      

    

